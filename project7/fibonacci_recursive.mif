DEPTH = 256;
WIDTH = 16;
ADDRESS_RADIX = HEX
DATA_RADIX = BIN
CONTENT
BEGIN
00 : 1111100000000000; -- RA = 0
01 : 1111100000101001; -- RB = 5 
02 : 1111111111111010; -- RC = -1 
03 : 0011010000000110; -- Call fib
04 : 0110000000000000; -- output RA 
05 : 0010000000010110; -- jump to last line 
06 : 0100001000000000; -- This is fib; save RB
07 : 0100011000000000; -- save RD 
08 : 1111000100000001; -- test if RB is zero
09 : 0011000000010011; -- if zero; jump to RETURN1
0A : 1000001010000100; -- test if RB-1 is zero 
0B : 0011000000010011; -- if zero jump to RETURN1
0C : 1000001010000001; -- RB = RB - 1
0D : 0011010000000110; -- call fib 
0E : 1000001010000001; -- RB = RB - 1
0F : 1111000000000011; -- store the result in RD
10 : 0011010000000110; -- call fib
11 : 1000000011000000; -- RA = RA + RD
12 : 0101011000000000; -- pop RD 
13 : 0101001000000000; -- pop RB 
14 : 0011100000000000; -- return 
15 : 1111100000001000; -- this is RETURN1; RA = 1
16 : 0101011000000000; -- pop RD
17 : 0101001000000000; -- pop RB 
18 : 0011100000000000; -- return 
19 : 0011110000000000; -- this is the last line; exit
[1A..FF] : 1111111111111111;
END
